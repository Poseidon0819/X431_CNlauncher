            <LinearLayout android:orientation="vertical" android:id="@+id/user_heavy_serial_number" android:background="@drawable/mine_info_item_bg" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:textSize="@dimen/leftmenu_textsize" android:textColor="@color/black" android:id="@+id/tv_user_heavy_serial_number" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/mine_area_margin" android:text="@string/mine_tv_user_heavy_device"/>
