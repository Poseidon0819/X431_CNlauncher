<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" android:background="@color/light_gray" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:gravity="center" android:id="@+id/btn_logo_image" android:background="@drawable/mine_info_item_bg" android:layout_width="@dimen/report_item_logo" android:layout_height="@dimen/report_item_logo" android:layout_marginTop="@dimen/mine_area_margin" android:layout_centerHorizontal="true">
            <TextView android:textSize="@dimen/leftmenu_textsize" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:id="@+id/pop_tv01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/diagnose_report_logo"/>
            <ImageView android:id="@+id/pop_iv01" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/mine_btnline" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_below="@+id/btn_logo_image">
            <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_garage_name" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginRight="@dimen/register_itemt_check" android:hint="@string/print_garage_name_txt" android:imeOptions="actionNext" style="@style/setting_edittext_style"/>
                <CheckBox android:layout_gravity="center_vertical" android:id="@+id/cb_garage_name" android:background="@drawable/select_btn_cbox_check" android:visibility="invisible" android:layout_width="@dimen/register_itemt_check" android:layout_height="wrap_content" android:checked="true" android:button="@null" android:layout_alignBaseline="@+id/edit_garage_name" android:layout_alignParentRight="true"/>
                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_garage_address" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginTop="@dimen/mine_area_margin" android:layout_marginRight="@dimen/register_itemt_check" android:hint="@string/print_company_address_txt" android:layout_below="@+id/edit_garage_name" android:imeOptions="actionNext" style="@style/setting_edittext_style"/>
                <CheckBox android:layout_gravity="center_vertical" android:id="@+id/cb_garage_address" android:background="@drawable/select_btn_cbox_check" android:visibility="invisible" android:layout_width="@dimen/register_itemt_check" android:layout_height="wrap_content" android:checked="true" android:button="@null" android:layout_alignBaseline="@+id/edit_garage_address" android:layout_alignParentRight="true"/>
                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_telephone" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginTop="@dimen/mine_area_margin" android:layout_marginRight="@dimen/register_itemt_check" android:hint="@string/print_phonenumber_txt" android:numeric="integer" android:layout_below="@+id/edit_garage_address" android:imeOptions="actionNext" style="@style/setting_edittext_style"/>
                <CheckBox android:layout_gravity="center_vertical" android:id="@+id/cb_telephone" android:background="@drawable/select_btn_cbox_check" android:visibility="invisible" android:layout_width="@dimen/register_itemt_check" android:layout_height="wrap_content" android:checked="true" android:button="@null" android:layout_alignBaseline="@+id/edit_telephone" android:layout_alignParentRight="true"/>
                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_fax" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginTop="@dimen/mine_area_margin" android:layout_marginRight="@dimen/register_itemt_check" android:hint="@string/print_fax_txt" android:layout_below="@+id/edit_telephone" android:imeOptions="actionDone" style="@style/setting_edittext_style"/>
                <CheckBox android:layout_gravity="center_vertical" android:id="@+id/cb_fax" android:background="@drawable/select_btn_cbox_check" android:visibility="invisible" android:layout_width="@dimen/register_itemt_check" android:layout_height="wrap_content" android:checked="true" android:button="@null" android:layout_alignBaseline="@+id/edit_fax" android:layout_alignParentRight="true"/>
                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_license_plate_number" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:visibility="gone" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginTop="@dimen/mine_area_margin" android:layout_marginRight="@dimen/register_itemt_check" android:hint="@string/print_license_plate_number_txt" android:singleLine="true" android:layout_below="@+id/edit_fax" android:imeOptions="actionDone" style="@style/setting_edittext_style"/>
                <CheckBox android:layout_gravity="center_vertical" android:id="@+id/cb_plate_number" android:background="@drawable/select_btn_cbox_check" android:visibility="invisible" android:layout_width="@dimen/register_itemt_check" android:layout_height="wrap_content" android:checked="true" android:button="@null" android:layout_alignBaseline="@+id/edit_license_plate_number" android:layout_alignParentRight="true"/>
                <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/mine_area_margin" android:layout_below="@+id/edit_license_plate_number">
                    <Button android:textColor="@color/white" android:id="@+id/btn_save" android:layout_width="match_parent" android:layout_height="@dimen/mine_item_height" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginRight="@dimen/register_itemt_check" android:text="@string/print_save_txt" android:layout_weight="1" style="?attr/commonButtonLongStyle"/>
                    <Button android:textColor="@color/white" android:id="@+id/btn_cancel" android:layout_width="match_parent" android:layout_height="@dimen/mine_item_height" android:layout_marginLeft="@dimen/register_itemt_check" android:layout_marginRight="@dimen/register_itemt_check" android:text="@string/common_cancel" android:layout_weight="1" style="?attr/commonButtonLongStyle"/>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
        <LinearLayout android:background="@color/light_gray" android:layout_width="match_parent" android:layout_height="100dp" android:layout_below="@+id/mine_btnline"/>
    </RelativeLayout>
</ScrollView>
