<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270" android:centerY="0.75" android:centerColor="#ffffffff"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270" android:centerY="0.75" android:centerColor="#ffffffff"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#f99d4a" android:endColor="#f99d4a" android:angle="270" android:centerY="0.75" android:centerColor="#f99d4a"/>
            </shape>
        </clip>
    </item>
</layer-list>
