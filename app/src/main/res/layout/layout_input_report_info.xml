            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
                <com.cnlaunch.x431pro.widget.MarqueeScrollView android:ellipsize="marquee" android:gravity="center" android:paddingRight="@dimen/dp_10" android:layout_width="0dp" android:layout_height="match_parent" android:text="@string/diagnose_report_carname" android:singleLine="true" android:layout_weight="1" style="@style/setting_edittext_style"/>
