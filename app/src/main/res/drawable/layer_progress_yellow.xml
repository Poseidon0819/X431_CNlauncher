<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ffffad00" android:endColor="#ffffad00" android:angle="270" android:centerY="0.75" android:centerColor="#ffffad00"/>
            <stroke android:width="1px" android:color="#ffad00"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#ffffad00" android:endColor="#ffffad00" android:angle="270" android:centerY="0.75" android:centerColor="#ffffad00"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <stroke android:width="5px" android:color="@android:color/transparent"/>
                <solid android:color="#ffad00"/>
            </shape>
        </clip>
    </item>
</layer-list>
