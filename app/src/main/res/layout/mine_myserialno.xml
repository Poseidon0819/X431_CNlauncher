                <com.cnlaunch.x431pro.widget.ListViewForScrollView android:id="@+id/lv_user_car_and_heavy_serial_number" android:paddingLeft="@dimen/dp_10" android:fadingEdge="none" android:layout_width="match_parent" android:layout_height="wrap_content" android:dividerHeight="0.01dp"/>
