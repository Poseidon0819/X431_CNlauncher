                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_car_remark" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:paddingRight="@dimen/dp_10" android:layout_width="0dp" android:layout_height="70dp" android:hint="@string/diagloghistorydetail_remark" android:layout_weight="3" android:inputType="textMultiLine" android:imeOptions="actionDone" style="@style/setting_edittext_style"/>
