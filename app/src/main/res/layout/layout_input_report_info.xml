                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_car_tester_name" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:paddingRight="@dimen/dp_10" android:layout_width="0dp" android:layout_height="match_parent" android:hint="@string/diagnose_report_tester" android:layout_weight="3" android:imeOptions="actionNext" style="@style/setting_edittext_style"/>
