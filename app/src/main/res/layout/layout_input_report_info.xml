                <com.cnlaunch.x431pro.widget.ClearEditText android:id="@+id/edit_car_year" android:background="@drawable/mine_info_item_bg" android:paddingLeft="@dimen/edit_padding_left" android:paddingRight="@dimen/dp_10" android:layout_width="0dp" android:layout_height="match_parent" android:hint="@string/my_customer_car_year" android:maxLength="20" android:layout_weight="3" android:imeOptions="actionNext" style="@style/setting_edittext_style"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
