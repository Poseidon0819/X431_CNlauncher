        <LinearLayout android:orientation="vertical" android:id="@+id/other_serial_number" android:background="@drawable/mine_info_item_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/mine_area_margin">
            <TextView android:textSize="@dimen/leftmenu_textsize" android:textColor="@color/black" android:id="@+id/tv_local_serial_number" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="@dimen/mine_area_margin" android:text="@string/mine_tv_other_device"/>
            <com.cnlaunch.x431pro.widget.ListViewForScrollView android:id="@+id/lv_local_serial_number" android:paddingLeft="@dimen/dp_10" android:fadingEdge="none" android:layout_width="match_parent" android:layout_height="wrap_content" android:dividerHeight="0.01dp"/>
            <LinearLayout android:orientation="vertical" android:id="@+id/other_heavy_serial_number" android:background="@drawable/mine_info_item_bg" android:layout_width="match_parent" android:layout_height="wrap_content">
